[Unit]
Description=Open-source Telegram VFS
[Service]
WorkingDirectory=/content/tgcloud
#ExecStart=/bin/python dedupfs/dedupfs.py -f --block-size 20971520 -o auto_unmount -o direct_io -o no_splice_write -o no_splice_read -o no_splice_move storage/
ExecStart=/bin/python dedupfs/dedupfs.py -f --block-size 20971520 -o hard_remove -o auto_unmount storage/
Restart=always
RestartSec=10
SyslogIdentifier=tgcloud-dedupfs-fuse
User=root
[Install]
WantedBy=multi-user.target
